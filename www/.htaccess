<IfModule mod_rewrite.c>
RewriteEngine on
RewriteBase /
RewriteCond %{REQUEST_FILENAME} !-d
#RewriteRule   ^([^/]*)  $1/  [R]
RewriteRule    ^([^/]*)/	index.php/Home/cate/index/url/$1 [L]

RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_FILENAME} !-f
RewriteRule   ^(.*)$ index.php/$1 [QSA,PT,L]
</IfModule>
